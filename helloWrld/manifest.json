{
  "manifest_version": 2,
  "name": "Leon's extension",
  "description": "My first extension",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "pic.png",
    "default_title": "heyy"
  }
}
